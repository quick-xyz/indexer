# Environment Configuration Template
# Copy this file to .env and fill in your actual values

# ========================================
# Environment Mode & Logging
# ========================================
ENVIRONMENT=development  # "development" for dev, "production" for prod
LOG_LEVEL=INFO          # DEBUG, INFO, WARNING, ERROR
INDEXER_LOG_DIR="./logs"
INDEXER_LOG_CONSOLE=True
INDEXER_LOG_FILE=True
INDEXER_LOG_STRUCTURED=True


# ========================================
# GCP Configuration
# ========================================
INDEXER_GCP_PROJECT_ID=gcp-project-id  # Replace with your GCP project ID
INDEXER_GCS_BUCKET_NAME=gcs-bucket-name  # Replace with your GCS bucket name
INDEXER_SQL_CONNECTION_NAME="indexerxyz:us-central1:indexerxyz-postgres"
INDEXER_DB_PORT=5432
INDEXER_DB_NAME=indexer_model_database # Replace with the database for the indexer model
INDEXER_DB_HOST=127.0.0.1  # Local development via proxy. Remove for production (uses secret).


